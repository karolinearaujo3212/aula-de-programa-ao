programa {
  funcao inicio() {
    real s1, s2, d3, m4, m5, x, y, z, c
    escreva ("digite um valor")
    leia (s1)
    escreva ("digite o valor de soma")
    leia (s2)
    escreva ("digite o valor para subtrair o valor somado")
    leia (d3)
    escreva ("digite o valor para multiplicar o numero subtraido")
    leia (m4)
    escreva ("digite o valor para dividir o numero subtraido")
    leia (m5)
    x=s1+s2
    y=x-d3
    z=y*m4
    c=z/m5
    escreva ("o cauculo do usuario e=", c)
  }
}
