programa {
  funcao inicio() {
    real n1, n2, n3, n4, n
    escreva ("digite a primeira media de nota")
    leia (n1)
    escreva ("digite a segunda media de nota")
    leia (n2)
    escreva ("digite a terceira media de nota")
    leia (n3)
    escreva ("digite a quarta media de nota")
